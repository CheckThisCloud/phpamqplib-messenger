{
    "name": "checkthiscloud/phpamqplib-messenger",
    "type": "symfony-bundle",
    "description": "Symfony messenger transport for the php-amqplib/php-amqplib library.",
    "keywords": [
        "symfony",
        "messenger",
        "enqueue"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "php-amqplib/php-amqplib": "^3.7",
        "phpstan/phpstan-phpunit": "^2.0",
        "symfony/amqp-messenger": "^7.4 || ^8.0",
        "symfony/framework-bundle": "^7.4 || ^8.0",
        "symfony/messenger": "^7.4 || ^8.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^14.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.1",
        "symfony/phpunit-bridge": "^7.2 || ^8.0",
        "symfony/uid": "^7.2 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jwage\\PhpAmqpLibMessengerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwage\\PhpAmqpLibMessengerBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable"
}
